<div>
  <h1>{{model.title}}</h1>
  <span>{{model.date}}</span> - <span>Client: {{model.client}}</span>
  <div>
    {{#each model.category as |category|}}
      <span class="project-item__category {{category}}">{{category}}</span>
    {{/each}}
  </div>
  <p>{{model.description}}</p>
</div>

{{outlet}}
