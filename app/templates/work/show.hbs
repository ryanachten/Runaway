<div>
  {{#video-vendor-player class="project-page-player" url=model.videoVendorUrl}}
    <div class="project-page--video-inactive">
      <div class="project-page--info">
        <h2 class="project-item__title">{{model.title}}</h2>
        <article class="project-item__meta">
          <span>{{model.date}}</span>
          <hr class="project-item__divider" />
          <span>{{model.client}}</span>
        </article>
      </div>
      {{!-- <p class="project-page__description">{{model.description}}</p> --}}
      <svg class="video-vendor__play-button" xmlns="http://www.w3.org/2000/svg" height="100" width="100">
        <path class="circle" fill="none" stroke="#fff" d="M49.9 2.5C23.6 2.8 2.1 24.4 2.5 50.4 2.9 76.5 24.7 98 50.3 97.5c26.4-.6 47.4-21.8 47.2-47.7-.2-26.1-21.8-47.5-47.6-47.3"/>
        <path class="triangle" fill="#fff" d="M38 69c-1 .5-1.8 0-1.8-1.1V32.1c0-1.1.8-1.6 1.8-1.1l34 18c1 .5 1 1.4 0 1.9L38 69z"/>
      </svg>
    </div>
  {{/video-vendor-player}}
</div>

{{outlet}}
